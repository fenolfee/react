{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "b": "pnpm --filter @react/backend",
    "w": "pnpm --filter @react/webapp",
    "dev": "pnpm -r --parallel dev",
    "types": "concurrently \"pnpm b types\" \"pnpm w types\"",
    "prettify": "pnpm -r --parallel prettier",
    "prepare": "husky"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
